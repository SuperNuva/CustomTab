{
    "name": "CustomTab",
    "version": "1.0",
    "description": "Get a customized browser when you open a new tab",
    "permissions": ["activeTab"],

    "browser_action": {
        "default_title": "Custom Tab"
    },

    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    
    "manifest_version": 2
}